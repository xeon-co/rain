page{
  width: 100%;
  height: 100%;
  /** verflow:hidden 超出部分隐藏 
  */ 
  overflow: hidden;
}
.page{
  background: white;
  height: 100%;
  overflow: hidden;
}
.head{
  width:100%;
    height:100px;
    background:-webkit-gradient(linear, 0 0, right bottom, from(#ff7e5e), to(#fe473c));  
}
.whiteCol{
  color: white;
}
.font14{
  font-size: 14px;
}
.p20{
  padding: 50rpx;
  box-sizing: border-box;
}
.mt20{
  margin-top: 20rpx;
}
.inputAreaSty{
  width:10rpx;
  height:60rpx;
  float:left;
  margin:-6rpx 0rpx;
  padding:2px 30rpx!important;
}
.inputNumSty{
  width: 70rpx;
  height: 73rpx; 
  line-height: 40px;
  border: 1px solid #999;
  border-radius: 6px;
  float: left;
  margin: 0 1px; 
  /* padding:10rpx 30rpx!important; */
}
.inputBox{
  width: 85%;
  margin: 0 auto;
  text-align: center;
  overflow: hidden;
}
.areaBox{
  border: 1px solid #999;
  width: 75rpx;
  height: 75rpx;
  line-height: 75rpx;
  float: left;
  border-radius: 6px;
  /* padding-top: 8rpx; */
  box-sizing: border-box
}
.middleLine{
  width: 0rpx;
  height: 40rpx;
  margin-top: 18rpx;
  border-left: 1rpx solid #999;
  float: left;
}
.separator{
  float:left;
  font-size: 80rpx;
  margin-top: -5rpx;
}
.areaTit{
  width: 100%;
}
.areaTit{
  margin: 30rpx 0 30rpx 58rpx;
  display: block;
  color:#999
}
.font18{
  font-size: 18px;
}
.font12{
  font-size: 12px;
}
.wid20{
  width: 20px;
}
.grayCol{
  color: #999
}
.addInput{
  width: 74rpx;
  height: 74rpx;
  float:left;
  display: inline-block;
  border: 1px dotted #999;
  border-radius: 6px;
  /* background: url(../../image/); */
  margin: 0 1px;
}
.addFn{
  width: 85%;
  margin: 50rpx auto;
  height: 80rpx;
  line-height: 82rpx;
  background:#fe7870;
  text-align: center;
  color:#feb0ab;
  border-radius: 16px;
}
.line{
  width: 85%;
  height: 1px;
  background: #ccc;
  margin: 0 auto;
}
.lineWord{
  width: 85%;
  margin: 0 auto;
  text-align: center;
  display: block;
  margin-top: -20rpx;
}
.lineWord text{
  background: #fff;
  padding: 0 10px;
  font-size: 12px;
  color:#999;
}
.actionWord{
  width: 85%;
  font-size: 14px;
  color:#999;
  margin: 25rpx auto;
}
.outFrame{
  width: 100%;
  height: 680rpx;
  position: absolute;
  bottom: -650rpx;
  background: #f6f6f6;
}
.topArea{
  width: 100%;
  height: 70rpx;
  background: white;
  text-align:right;
  padding-right:20rpx;
  box-sizing: border-box;
  color:#fe473c;
  font-size:14px;
  border-top:1px solid #ccc;
  line-height: 70rpx;
  overflow: hidden;
}
.choseArea{
  width:100%;
  height: 600rpx;
  overflow:hidden; 
  /* display: flex; */
  /* flex-wrap: wrap; */
    padding: 10rpx;  
  /* box-sizing: border-box; */
}
.choseArea li{
  list-style: none;
  width: 105rpx;  
  height: 65rpx;
  text-align: center;
  line-height: 65rpx;
  float: left;
  background: white;
  margin:10rpx 10rpx;
  /* flex: 12%; */
}
/* .choseArea li:first-child{
  margin-left: 20rpx;
}*/
.choseArea li:last-child{
  flex: 14%;  
  flex-grow: 0
}
.deleteBtn{
  width: 228rpx;
  height: 65rpx;
  background: white;
  position: absolute;
  right: 20rpx;
  bottom: 60rpx;
}
.animation-element-wrapper {
  display: flex;
  width: 100%;
  padding-top: 150rpx;
  padding-bottom: 150rpx;
  justify-content: center;
  overflow: hidden;
  background-color: #ffffff;
}
.animation-element {
  /* width: 200rpx;
  height: 200rpx; */
  /* background-color: #1AAD19; */
}
.animation-buttons {
  padding: 30rpx 50rpx 10rpx;
  width: 100%;
  height: 600rpx;
  box-sizing: border-box;
}
.animation-button {
  float: left;
  line-height: 2;
  width: 300rpx;
  margin: 15rpx 12rpx;
}

.animation-button-reset {
  width: 620rpx;
}
.mask_layer {  
  width: 100%;  
  height: 100%;  
  position: fixed;  
  z-index: 1000;  
  background: #000;  
  opacity: 0.5;  
  overflow: hidden;  
}  
.modal_box {  
  width: 65%;  
  overflow: hidden;  
  position: fixed;  
  top: 50%;  
  left: 0;  
  z-index: 1001;  
  background: #fafafa;  
  margin: -150px 17.5% 0 17.5%;  
  border-radius:9px;
}  
  
.title {  
  padding: 15px;  
  text-align: center;  
  background-color: gazure;  
}  
  
.content {  
  overflow-y: scroll; /*超出父盒子高度可滚动*/  
}  
  
.btn {  
  width: 100%;  
  margin-top: 30rpx;  
  display: flex;  
  flex-direction: row;  
  align-items: center;  
  justify-content: space-between;  
  box-sizing: border-box;  
  background-color: white;  
}  
button{
  color: #66c15e;
  font-size: 16px;
}
  
.cancel {  
  width: 100%;  
  /* padding: 10px;   */
  text-align: center;  
  background: #fafafa;  
  border-top:1px solid #ccc;
  border-right:1px solid #ccc;  
}  
  
.Sure {  
  width: 100%;  
  padding: 10px;  
  /* background-color: gainsboro;   */
