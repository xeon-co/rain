{
    "pages": [
        "pages/index/index",
        "pages/log/index",
        "pages/log/page",
        "selfcashiers/pages/index/index",
        "parking/pages/index/index"
    ],
    "subpackages": [
        {
          "root": "packageA",
          "name": "pack2",
          "pages": [
            "pages/log/page"
          ],
          "independent": true
        }
      ],

      "preloadRule": {
        "pages/log/page": {
          "network": "all",
          "packages": ["packageA"]
        }
      },
    "window": {
        "backgroundTextStyle": "light",
        "navigationBarBackgroundColor": "#000",
        "navigationBarTitleText": "Rain英语",
        "navigationBarTextStyle": "white",
        "pageOrientation": "portrait",
        "restartStrategy": "homePage"
    },
    "workers": "workers",
    "lazyCodeLoading": "requiredComponents",
    "tabBar": {
    
      "list": [{
        "pagePath": "pages/index/index",
        "text": "首页"
      }, {
        "pagePath": "pages/log/index",
        "text": "日志"
      }]
    },
    "networkTimeout": {
      "request": 10000,
      "downloadFile": 10000
    },
    "debug": true,
    "style": "v2",
    "sitemapLocation": "sitemap.json"
}